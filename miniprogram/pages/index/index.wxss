.container {
    padding: 20px;
  }
  .search-bar {
    display: flex;
    margin-bottom: 20px;
  }
  .search-bar input {
    flex: 1;
    border: 1px solid #ccc;
    padding: 5px;
    border-radius: 4px;
  }
  .search-bar button {
    margin-left: 10px;
    padding: 5px 10px;
    background-color: #1AAD19;
    color: white;
    border: none;
    border-radius: 4px;
  }
  .project-list {
    display: flex;
    flex-direction: column;
  }
  .project-item {
    padding: 10px;
    border-bottom: 1px solid #eee;
    cursor: pointer;
  }
  .project-name {
    font-size: 16px;
    font-weight: bold;
  }
  .project-status {
    font-size: 14px;
    color: gray;
  }
  